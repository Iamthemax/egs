<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_dashboard">
    <fragment
        android:id="@+id/navigation_sync"
        android:name="com.sumagoinfotech.digicopy.ui.fragments.dashboard.sync.SyncOfflineData"
        android:label="@string/sync"
        tools:layout="@layout/activity_allocate_work" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.sumagoinfotech.digicopy.ui.fragments.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <activity
        android:id="@+id/navigation_attendence"
        android:name="com.sumagoinfotech.digicopy.ui.activities.AllocateWorkActivity"
        tools:layout="@layout/activity_allocate_work" />

    <activity
        android:id="@+id/navigation_document"
        android:name="com.sumagoinfotech.digicopy.ui.activities.DocumentPagesActivity"
        tools:layout="@layout/activity_document_pages" />

    <activity
        android:id="@+id/navigation_reports"
        android:name="com.sumagoinfotech.digicopy.ui.activities.ReportsActivity"
        android:label="@string/title_notifications"
        tools:layout="@layout/activity_labour_details" />


</navigation>